#include <stdio.h>
int main() {
    int type, mins, charge = 0;
    scanf("%d %d", &type, &mins);

    switch(type) {
        case 1: charge = mins * 1; break;
        case 2: charge = mins * 3; break;
        case 3: charge = mins * 10; break;
    }

    printf("Call Charge â‚¹%d", charge);
    return 0;
}
